#include<stdio.h>

int main()
{
        float c=0,f=0;
        printf("conversion of temperature\n");
        printf("enter the temperature in celsius\n");
        scanf("%f",&c);
        f=(c*9/5)+32;
        printf("the temperature in fahrenheit is %f\n",f);
        return 0;
}
                                                                                                                                      
                                                                                                                                      
                                                                                                                                      
                                                                                                                                    
                        
